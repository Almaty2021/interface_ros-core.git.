<launch>
	<arg name="veh"/>
	<arg name="pkg_name" value="camera_driver"/>
	<arg name="node_name" default="camera_node"/>
	<arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman"/>

	<group ns="$(arg veh)">
		<node  name="camera_node" pkg="$(arg pkg_name)" type="camera_node_sequence.py" output="screen" clear_params="true" required="true">
			<rosparam command="load" file="$(find $(arg pkg_name))/config/$(arg node_name)/$(arg param_file_name).yaml"/>
		</node>
	</group>

	<!-- Publications -->
	<!-- "~image/compressed": sensor_msgs/CompressedImage. Image from the camera in jpeg format-->

</launch>
