<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="node_name" default="wheels_driver_node"/>
    <arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman." />

    <node ns="$(arg veh)"  pkg="wheels_driver" type="$(arg node_name).py" name="$(arg node_name)" output="screen" clear_params="true" required="true">
        <rosparam command="load" file="$(find wheels_driver)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
    </node>

    <!-- Subscriptions -->
    <!-- "~wheels_cmd": duckietown_msgs/WheelsCmd. Command for the wheels -->

</launch>
